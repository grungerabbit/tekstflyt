<div class='non-header-page'>
	<h3>Flows for <a href='/w/{{ writer }}'>{{ writer }}</a></h3>
	<ul>
	{% for flow in flows %}
	    <li><a href="/flow/{{flow.number}}/view">{{ flow.title }}</a></li>
	{% endfor %}
	{% if flows.size < 1 %}
	<p>There are no flows at this time.</p>
	{% endif %}
	</ul>
</div>